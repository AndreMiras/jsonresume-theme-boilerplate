	</style>
	
	</head>
	<body>
	
	<div id="page">

        {{#resume.basics}}
        <div id="top-contact">
            <div class="contact-details">
                {{#if email}}
                <div class="fa fa-envelope"></div> {{email}}<br />
                {{/if}}
                {{#if phone}}
                <div class="fa fa-phone"></div> {{phone}}<br />
                {{/if}}
                {{#if website}}
                <div class="fa fa-globe"></div><a href="{{website}}"> {{website}}</a>
                {{/if}}
            </div>
        </div>

        <h1 class="emphnext">{{name}}</h1><br/>
        {{#stars}}
        <span class="stars">
            {{#items}}
            {{name}} {{^last}}<div class="icon-star-o icon-star"></div>{{/last}}
            {{/items}}
        </span>
        {{/stars}}
        <br/>

        {{#if summary}}
        <h2><div class="fa fa-user"></div> Qualifications Summary</h2>
        <!-- TODO -->
        <!-- ul>
            {{#points}}
            <li>{{{.}}}</li>
            {{/points}}
        </ul -->
		<div class="summary">
			<p>{{summary}}</p>
		</div>
        {{/if}}

        {{#if skills}}
        <h2><div class="icon-th-list icon-square"></div> Technical Skills</h2>
        <ul class="languages">
            {{#details}}
            <li>{{type}}:
                {{#items}}
                <span class="label label-default">{{name}}</span>
                {{/items}}
            </li>
            {{/details}}
        </ul>
        {{/if}}

