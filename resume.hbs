	<!-- /section -->
	{{/resume.basics}}

    {{#if resume.work.length}}
	<h2><div class="fa fa-briefcase"></div> Professional Experience</h2>
	<ul>
        {{#each resume.work}}
        <!-- TODO: location -->
		<li>{{position}}, {{#if website}}<a href="{{website}}">{{company}}</a>{{else}}{{company}}{{/if}}{{#if location}}, {{{location}}}{{/if}}
            <span class="right">({{#if startDate}}{{startDate}} – {{/if}}{{#if endDate}}{{endDate}}{{else}}Present{{/if}})</span>
            {{#if summary}}
            <p>{{ summary }}</p>
            {{/if}}
			<ul>
				{{#if highlights.length}}
                {{#each highlights}}
				<li>{{{.}}}</li>
				{{/each}}
                <!-- TODO: technology_used -->
				{{#technology_used}}
				<li>##technology_used##:
					{{#tools}}
					<span class="label label-default">{{name}}</span>
					{{/tools}}
				</li>
				{{/technology_used}}
			</ul>
            {{/if}}
		</li>
		{{/each}}
	</ul>
	{{/if}}

    {{#if resume.volunteer.length}}
	<h2><div class="fa fa-hand-paper-o"></div> Professional Experience</h2>
	<ul>
        {{#each resume.volunteer}}
        <!-- TODO: location -->
		<li>{{position}}, {{#if website}}<a href="{{website}}">{{organization}}</a>{{else}}{{organization}}{{/if}}{{#if location}}, {{{location}}}{{/if}}
            <span class="right">({{#if startDate}}{{startDate}} – {{/if}}{{#if endDate}}{{endDate}}{{else}}Present{{/if}})</span>
            {{#if summary}}
            <p>{{ summary }}</p>
            {{/if}}
			<ul>
				{{#if highlights.length}}
                {{#each highlights}}
				<li>{{{.}}}</li>
				{{/each}}
                <!-- TODO: technology_used -->
				{{#technology_used}}
				<li>##technology_used##:
					{{#tools}}
					<span class="label label-default">{{name}}</span>
					{{/tools}}
				</li>
				{{/technology_used}}
			</ul>
            {{/if}}
		</li>
		{{/each}}
	</ul>
	{{/if}}

	{{#if resume.education.length}}
	<h2><div class="fa fa-graduation-cap"></div> Education</h2>
	<div id="mobile-table">
		<div class="table-responsive">
			<table class="table education">
				<tr>
                    <!-- TODO: handle show_gpa GPA -->
					<th>Degree</th><th>Major</th><th>Institution</th><th nowrap>Graduation Year</th><th>GPA</th>
				</tr>
				{{#each resume.education}}
				<tr>
					<td>{{studyType}}</td><td>{{area}}</td><td>{{institution}}</td><td align="center">{{endDate}}</td>{{#if gpa}}<td>{{gpa}}</td>{{/if}}
				</tr>
				{{/each}}
			</table>
		</div>
	</div>
	<div id="desktop-table">
		<table class="table education">
			<tr>
                <!-- TODO: handle show_gpa GPA -->
				<th>Degree</th><th>Major</th><th>Institution</th><th nowrap>Graduation Year</th><th>GPA</th>
			</tr>
			{{#each resume.education}}
			<tr>
				<td>{{studyType}}</td><td>{{area}}</td><td>{{institution}}</td><td align="center">{{endDate}}</td>{{#if gpa}}<td>{{gpa}}</td>{{/if}}
			</tr>
			{{/each}}
		</table>
	</div>
	{{/if}}

